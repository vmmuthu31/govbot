{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "console": "readonly",
    "URL": "readonly",
    "process": "readonly"
  },
  "ignorePatterns": ["lib/generated/**/*", ".next/**/*", "node_modules/**/*"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ],
    "@typescript-eslint/no-explicit-any": "warn"
  }
}
